{
	"compilerOptions": {
		"target": "es6",
		"lib": [
			"es2015",
			"dom"
		],
		"jsx": "react",
		"declaration": false,
		"sourceMap": true,
		"outDir": "dist",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"moduleResolution": "node",
		"baseUrl": "src",
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	}
}
