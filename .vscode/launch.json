{
    "version": "0.2.0",
    "configurations": [
		{
			"name": "Jest",
			"type": "node",
			"request": "launch",
			"env": { "NODE_ENV": "test" },
			"args": [
				"--config=jest/config.json"
			],
			"runtimeArgs": [
			  "--inspect-brk",
			  "--debug-brk",
			  "${workspaceRoot}/node_modules/.bin/jest",
			  "--runInBand"
			],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		  }
    ]
}
